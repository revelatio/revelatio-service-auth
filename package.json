  "license": "MIT",
  "dependencies": {
    "axios": "^0.15.3",
    "babel-runtime": "^6.23.0",
    "bluebird": "^3.5.0",
    "dotenv": "^4.0.0",
    "express": "^4.15.3",
    "github": "^9.2.0",
    "jsonwebtoken": "^7.4.1",
    "microrouter": "^2.1.1",
    "querystring": "^0.2.0",
    "ramda": "^0.24.0",
    "uid-promise": "^0.1.0"
  },
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-core": "^6.24.1",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-1": "^6.24.1",
    "mocha": "^3.3.0",
    "must": "^0.13.4",
    "must-sinon": "^0.0.0",
    "sinon": "^2.2.0"
