{
  "name": "revelatio-service-auth",
  "version": "0.1.1",
  "description": "Revelat.io Github auth service",
  "main": "lib/index.js",
  "scripts": {
    "deploy": "gcloud beta functions deploy revelatio-service-auth-0_1_1 --entry-point handler --trigger-http --stage-bucket revelatio-functions --memory=256MB",
    "build": "rm -rf lib/ && `yarn bin`/babel ./src --out-dir ./lib --ignore index-local.js",
    "test": "mocha --ui bdd --compilers js:babel-register --recursive"
